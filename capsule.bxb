capsule {
  id (hwasim.oiling_service)
  version (1.1.0)
  format (3)
  targets {
    target (bixby-mobile-ko-KR)
    target (bixby-mobile-en-US){enabled (false)}
  }
  runtime-flags {
    concepts-inherit-super-type-features
    modern-prompt-rejection
    support-halt-effect-in-computed-inputs
  }
  
  capsule-imports{
    import (viv.core) {
      as (core)
    }
    import (viv.geo) {
      as (geo)
      version (9.17.18)
    }
  }
  
  permissions{
    user-profile-access
  }
}
